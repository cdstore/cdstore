<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ProductCatalogService WSDL service -->

  <service name="ProductCatalog" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://catalogservice.cdstore.com"/>
      <parameter name="wsdlServiceElement" value="ProductCatalogService"/>
      <parameter name="schemaQualified" value="http://catalogservice.cdstore.com"/>
      <parameter name="wsdlServicePort" value="ProductCatalog"/>
      <parameter name="className" value="com.cdstore.catalogservice.ProductCatalogSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ProductCatalog"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getproduct" qname="operNS:getproduct" xmlns:operNS="http://catalogservice.cdstore.com" returnQName="retNS:getproductReturn" xmlns:retNS="http://catalogservice.cdstore.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:name" xmlns:pns="http://catalogservice.cdstore.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getproduct"/>

  </service>
</deployment>
